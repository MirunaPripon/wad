<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>ServletContextListener</description>
        <listener-class>listeners.ContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>LogInController</servlet-name>
        <servlet-class>controllers.LogInController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>controllers.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutController</servlet-name>
        <servlet-class>controllers.LogOutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddressController</servlet-name>
        <servlet-class>controllers.OrderNewController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderExistingController</servlet-name>
        <servlet-class>controllers.OrderExistingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckoutController</servlet-name>
        <servlet-class>controllers.CheckoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingController</servlet-name>
        <servlet-class>controllers.BookingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTableController</servlet-name>
        <servlet-class>controllers.AddTableController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddLocationController</servlet-name>
        <servlet-class>controllers.AddLocationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddFoodController</servlet-name>
        <servlet-class>controllers.AddFoodController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteFoodController</servlet-name>
        <servlet-class>controllers.DeleteFoodController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRegisterController</servlet-name>
        <servlet-class>controllers.AdminRegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSelectionsController</servlet-name>
        <servlet-class>controllers.DeleteSelectionsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteOrderController</servlet-name>
        <servlet-class>controllers.DeleteOrderController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogInController</servlet-name>
        <url-pattern>/LogInController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/RegisterController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutController</servlet-name>
        <url-pattern>/LogOutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddressController</servlet-name>
        <url-pattern>/AddressController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderExistingController</servlet-name>
        <url-pattern>/OrderExistingController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckoutController</servlet-name>
        <url-pattern>/CheckoutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingController</servlet-name>
        <url-pattern>/BookingController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTableController</servlet-name>
        <url-pattern>/AddTableController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddLocationController</servlet-name>
        <url-pattern>/AddLocationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddFoodController</servlet-name>
        <url-pattern>/AddFoodController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteFoodController</servlet-name>
        <url-pattern>/DeleteFoodController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRegisterController</servlet-name>
        <url-pattern>/AdminRegisterController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSelectionsController</servlet-name>
        <url-pattern>/DeleteSelectionsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteOrderController</servlet-name>
        <url-pattern>/DeleteOrderController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
